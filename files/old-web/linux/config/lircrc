    prog = irexec
    button = Mute
    config = xmms -u
end

begin
     prog = irexec
     button = PresUp
     config = mixctrl 0 64  ; (echo "0" > ./.lirc_old_vol)
end

begin
     prog = irexec
     button = PresDown
     config = mixctrl 0 0  ; echo "Zut"; (echo "64" > ./.lirc_old_vol)
end


